{
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "client/dist",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

